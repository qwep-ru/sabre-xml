{
    "name": "qwep-ru/sabre-xml",
    "description" : "Fork of sabre/xml",
    "keywords" : [ "XML", "XMLReader", "XMLWriter", "DOM" ],
    "homepage" : "https://sabre.io/xml/",
    "license" : "MIT",
    "require" : {
        "php" : ">=5.5",
        "ext-xmlwriter" : "*",
        "ext-xmlreader" : "*",
        "ext-dom" : "*",
        "lib-libxml" : ">=2.6.20",
        "sabre/uri" : ">=1.0,<3.0.0"
    },
    "support" : {
        "forum" : "https://groups.google.com/group/sabredav-discuss",
        "source" : "https://github.com/fruux/sabre-xml"
    },
    "autoload" : {
        "psr-4" : {
            "Sabre\\Xml\\" : "lib/"
        },
        "files": [
            "lib/Deserializer/functions.php",
            "lib/Serializer/functions.php"
        ]
    },
    "autoload-dev" : {
        "psr-4" : {
            "Sabre\\Xml\\" : "tests/Sabre/Xml/"
        }
    },
    "require-dev": {
        "sabre/cs": "~1.0.0",
        "phpunit/phpunit" : "*"
    },
    "config" : {
        "bin-dir" : "bin/"
    }
}
